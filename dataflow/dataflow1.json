				"          BldgType as string,",
				"          HouseStyle as string,",
				"          OverallQual as integer,",
				"          OverallCond as integer,",
				"          YearBuilt as integer,",
				"          YearRemodAdd as integer,",
				"          RoofStyle as string,",
